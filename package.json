{
  "name": "aws-lambda-events",
  "version": "0.0.1",
  "description": "Lightweight helpers to normalize API Gateway proxy events (v1.0 and v2.0) for AWS Lambda.",
  "target": "ES2019",
  "module": "CommonJS",
  "moduleResolution": "node",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "eslint */*.ts",
    "test": "jest --coverage --coverageReporters='text-summary' --silent",
    "coverage": "jest --coverage --coverageReporters=lcov --silent",
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianhsu0116/aws-lambda-events.git"
  },
  "keywords": [
    "aws",
    "lambda",
    "apigateway"
  ],
  "author": "Ian Hsu",
  "license": "MIT",
  "dependencies": {
    "aws-lambda": "^1.0.7"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.159",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.4.0"
  }
}